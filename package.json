    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.2",
    "exit-hook": "^4.0.0",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
